<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <!--矩形背景颜色-->
        <shape
            android:shape="rectangle">
            <solid android:color="#1e1e31"/>
            <!-- 边框的宽度和颜色-->
            <stroke
                android:width="1dp"
                android:color="#d1e2eb"/>
            <!-- 边框的圆角的弧度-->
            <corners android:radius="2dp"/>
        </shape>
    </item>
</layer-list>